{
  "name": "strands",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "accounts": "yarn run hardhat accounts",
    "compile": "yarn run hardhat compile",
    "clean": "rimraf build coverage typechain export coverage.json artifacts cache cache_forge out",
    "console": "yarn run hardhat console",
    "start": "yarn run hardhat node",
    "run": "yarn run hardhat run",
    "guide": "yarn run hardhat help",
    "test:hardhat": "yarn run hardhat test",
    "test:hardhatGas": "REPORT_GAS=true yarn run hardhat test",
    "test:forge": "forge test",
    "deploy:local": "yarn run hardhat deploy --network localhost",
    "deploy:linea": "yarn run hardhat deploy --network linea",
    "node-deploy:local": "TS_NODE_FILES=true yarn run ts-node src/scripts/deploy.ts",
    "eslint:check": "yarn run eslint '**/*.{js,ts}'",
    "eslint:fix": "yarn run eslint '**/*.{js,ts}' --fix",
    "prettier:check": "yarn run prettier '**/*.{json,sol,md}' --check",
    "prettier:fix": "yarn run prettier '**/*.{json,sol,md}' --write",
    "solhint:check": "yarn run solhint 'contracts/**/*.sol'",
    "solhint:fix": "yarn run solhint 'contracts/**/*.sol' --fix",
    "export-abi": "yarn run hardhat export-abi",
    "build": "run-s clean compile export-abi",
    "preRelease": "run-s eslint:fix solhint:fix prettier:fix",
    "remixd": "remixd -s $INIT_CWD --remix-ide https://remix.ethereum.org"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-foundry": "^1.0.2",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "^1.0.0",
    "@typechain/ethers-v6": "^0.4.0",
    "@typechain/hardhat": "^8.0.0",
    "@types/chai": "^4.2.0",
    "@types/mocha": ">=9.1.0",
    "@types/node": ">=16.0.0",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "chai": "^4.2.0",
    "config": "^3.3.9",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-node": "^0.3.7",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "ethers": "^6.4.0",
    "hardhat": "^2.14.0",
    "hardhat-abi-exporter": "^2.10.1",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-deploy": "^0.11.34",
    "hardhat-gas-reporter": "^1.0.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.1",
    "prettier-plugin-solidity": "^1.1.3",
    "rimraf": "^5.0.1",
    "solhint": "^3.4.1",
    "solidity-coverage": "^0.8.4",
    "ts-node": ">=8.0.0",
    "typechain": "^8.1.0",
    "typescript": ">=4.5.0"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.9.3",
    "@openzeppelin/test-helpers": "^0.5.16",
    "@remix-project/remixd": "^0.6.16"
  }
}
